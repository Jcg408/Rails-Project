
<div class= 'container'>
<h1>Log In </h1>
    <%= form_for @client, url: login_path do |f| %>
   
    <%= f.label :email, "Email" %>
    <%= f.text_field :email %> <br>

    <%= f.label :password, "Password" %>
    <%= f.password_field :password %> <br>

    <%= f.submit "Log In"%>
    <% end %>
</div>