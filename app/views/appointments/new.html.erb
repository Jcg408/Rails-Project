<h1>Appointments#new</h1>

<%= form_for @appointment do |f| %>
  <%= f.date_select :appointment_date %> <br> <br>
  <%= f.time_select :appointment_time %> <br> <br>
  <%= f.collection_select :staff, Staff.all, :id, :name %> <br> <br>
  <%= f.collection_select :service, Service.all, :id, :name %> <br> <br>
  <%= f.submit %>
<% end %>

