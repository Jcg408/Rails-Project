<h1>Sign Up</h1>

<p>
<%= form_for @client do |f| %>
    <%= f.label :name, "Name" %>
    <%= f.text_field :name %> <br>
    <%= f.label :email, "Email" %>
    <%= f.text_field :email %> <br>
    <%= f.label :password, "Password" %>
    <%= f.password_field :password %>

    <%= f.submit %>
<% end %>
</p>
